{
    "name": "@iocast/aurelia-mdc-plugin",
    "version": "0.5.8",
    "description": "Material Design Components plugin for Aurelia.",
    "keywords": [
        "aurelia",
        "plugin",
        "mdc",
        "material design"
    ],
    "scripts": {
        "build": "gulp build",
        "prepush": "gulp prepare-release --bump=patch",
        "push": "git add --all && git commit -m \"$(date)\" && git push origin master && npm publish --access public",
        "test": "gulp test"
    },
    "homepage": "https://github.com/iocast/aurelia-mdc-plugin",
    "bugs": {
        "url": "https://github.com/iocast/aurelia-mdc-plugin/issues"
    },
    "license": "MIT",
    "author": "Rabah Ghodbane <r-ghodbane@live.com>",
    "main": "dist/commonjs/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/iocast/aurelia-mdc-plugin"
    },
    "peerDependencies": {
        "material-components-web": "*"
    },
    "devDependencies": {
        "aurelia-tools": "^2.0.0",
        "autoprefixer": "^8.0.0",
        "babel-eslint": "^8.2.2",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
        "babel-plugin-transform-es2015-modules-systemjs": "^6.24.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-1": "^6.24.1",
        "conventional-changelog": "^1.1.16",
        "del": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.1",
        "gulp-bump": "^3.1.0",
        "gulp-eslint": "^4.0.2",
        "gulp-postcss": "^7.0.1",
        "gulp-yuidoc": "^0.1.2",
        "isparta": "^4.0.0",
        "istanbul": "^1.1.0-alpha.1",
        "jasmine-core": "^2.99.1",
        "karma": "^2.0.0",
        "karma-babel-preprocessor": "^7.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.1",
        "karma-jasmine": "^1.1.1",
        "karma-jspm": "2.2.3",
        "object.assign": "^4.1.0",
        "postcss-cssnext": "^3.1.0",
        "require-dir": "^1.0.0",
        "run-sequence": "^2.2.1",
        "vinyl-paths": "^2.1.0",
        "yargs": "^11.0.0"
    },
    "jspm": {
        "registry": "npm",
        "main": "index",
        "format": "amd",
        "directories": {
            "dist": "dist/amd"
        },
        "devDependencies": {
            "aurelia-framework": "^1.0.0",
            "aurelia-polyfills": "^1.0.0"
        }
    },
    "aurelia": {
        "build": {
            "resources": [
                "mdc-target"
            ]
        }
    },
    "dependencies": {
        "aurelia-framework": "^1.1.5"
    }
}
